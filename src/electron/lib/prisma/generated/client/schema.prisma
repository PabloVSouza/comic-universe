generator client {
  provider      = "prisma-client-js"
  output        = "../src/electron/lib/prisma/generated/client"
  binaryTargets = ["native", "darwin", "darwin-arm64"]
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id   Int    @id @default(autoincrement())
  name String
}
