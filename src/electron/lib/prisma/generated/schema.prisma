generator client {
  provider = "prisma-client-js"
  output   = "../src/electron/lib/prisma/generated/"
}

datasource db {
  provider = "sqlite"
  url      = "file:./database.db"
}

model User {
  id          Int      @id @default(autoincrement())
  name        String
  dateCreated DateTime @default(now())
}
